{
  "version": "2017-02-28",
  "operation": "PutItem",
  "key": {
    "storeId": $util.dynamodb.toDynamoDBJson($ctx.args.input.storeId),
    "accountId": $util.dynamodb.toDynamoDBJson($ctx.args.input.accountId),
  },
  "attributeValues": $util.dynamodb.toMapValuesJson($ctx.args.input),
  "condition": {
    "expression": "attribute_not_exists(#storeId) AND attribute_not_exists(#accountId)",
    "expressionNames": {
      "#storeId": "storeId",
      "#accountId": "accountId",
    },
  },
}